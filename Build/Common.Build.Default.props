<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <OptimiziedBuild />
    <OptimiziedBuild Condition="'$(Configuration)'=='fre' or '$(Configuration)'=='fretest'">true</OptimiziedBuild>
  </PropertyGroup>
  <PropertyGroup Label="Configuration">
    <CharacterSet>Unicode</CharacterSet>
    <WholeProgramOptimization Condition="'$(OptimiziedBuild)'=='true'">true</WholeProgramOptimization>
  </PropertyGroup>
</Project> 
